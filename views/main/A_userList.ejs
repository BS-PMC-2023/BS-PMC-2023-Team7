<!--Test for how a page upgrading users could look-->
<body onload="info()">
  <% for(const user of users) { %>
  <!--List users-->
  <%- include('../partials/upgrade.ejs', {user: user}); %>
  <br />
  <% } %>
</body>

<% var text =message; %>
<script type="text/javascript">
  function info() {
    var message = "<%= text %>";
    if (!message) return;
    return alert(message);
  }
</script>
